{
  "manifest_version": 2,
  "name": "context-menu-selection-dom-demo",
  "description": "context-menu-selection-dom-demo",
  "version": "1.0",
  "background": {
    "scripts": [
      "context-menu.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "selection-sender.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ]
}